   "source": [
    "def dist(a,b,x,y):\n",
    "    return abs(a-x) + abs(b-y) - min(abs(a-x), abs(b-y))\n",
    "\n",
    "n = 9\n",
    "m = 4\n",
    "\n",
    "mrr = []\n",
    "for i in range(n):\n",
    "    row = []\n",
    "    for j in range(m):\n",
    "        row.append(dist(0,0,i,j))\n",
    "    mrr.append(row)"
   ]
