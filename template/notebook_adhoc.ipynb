    "LARGE = 10**5\n",
    "\n",
    "srr = []\n",
    "srr.append(str(LARGE))\n",
    "srr.append(\" \".join([\"1\"]*LARGE))\n",
    "for i in range(1, LARGE):\n",
    "    srr.append(\"{} {}\".format(i, i+1))\n",
    "srr = \"\\n\".join(srr)\n",
    "    \n",
    "with open(\"out.txt\", \"w\") as f:\n",
    "    f.write(srr)"
